{
    "version": 2,
    "builds": [{
        "src": "ecommerce/wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ecommerce/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "ecommerce.settings",
        "PYTHONPATH": "."
    }
} 